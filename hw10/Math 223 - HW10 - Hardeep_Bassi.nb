Cell[TextData[{
 "Now, we must perform asymptotic matching to obtain the overlapping \
solution: \n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         UnderscriptBox["lim", 
          RowBox[{
            StyleBox["X", "TI"], "\[Rule]", "\[Infinity]"}], 
          LimitsPositioning -> True], 
         SubscriptBox[
          StyleBox["y", "TI"], 
          RowBox[{
            StyleBox["i", "TI"], 
            StyleBox["n", "TI"]}]], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[Tilde]", 
         UnderscriptBox["lim", 
          RowBox[{
            StyleBox["x", "TI"], "\[Rule] ", "0"}], LimitsPositioning -> 
          True], 
         SubscriptBox[
          StyleBox["y", "TI"], 
          RowBox[{
            StyleBox["o", "TI"], 
            StyleBox["u", "TI"], 
            StyleBox["t", "TI"]}]], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[DoubleLongRightArrow]", 
         StyleBox["C", "TI"], "+", 
         StyleBox["e", "TI"], "+", "\[Epsilon]", 
         StyleBox["D", "TI"], "\[Tilde]", 
         StyleBox["e", "TI"], "+", "\[Epsilon]", 
         StyleBox["e", "TI"], "\[DoubleLongRightArrow]", 
         StyleBox["C", "TI"], "\[LongEqual]", "0", ",", 
         StyleBox["D", "TI"], "\[LongEqual]", 
         StyleBox["e", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\lim_{X\\to\\infty}y_{in}(x) \\sim \\lim_{x\\to\\ 0}y_{out}(x) \
\\implies C + e + \\epsilon D \\sim e + \\epsilon e \\implies C = 0, D = e", 
     "state" -> "Boxes"|>,
