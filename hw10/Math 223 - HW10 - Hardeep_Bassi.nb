Cell[TextData[{
 "Hence, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["y", "TI"], 
          RowBox[{
            StyleBox["o", "TI"], 
            StyleBox["u", "TI"], 
