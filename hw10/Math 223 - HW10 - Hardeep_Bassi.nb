         RowBox[{"(", "0", ")"}], "\[Tilde]", 
         UnderscriptBox["lim", 
          RowBox[{
            StyleBox["x", "TI"], "\[Rule]", "0"}], LimitsPositioning -> True], 
         SubscriptBox[
          StyleBox["y", "TI"], 
          RowBox[{
            StyleBox["o", "TI"], 
            StyleBox["u", "TI"], 
            StyleBox["t", "TI"]}]], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[DoubleLongRightArrow]", 
         StyleBox["C", "TI"], 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          RowBox[{"-", 
