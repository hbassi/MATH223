Cell[TextData[{
 "Taking a series expansion of the integrand and then integrating, allows us \
to obtain the leading behavior as ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["x", "TI"], "\[Rule]", 
         SuperscriptBox["0", "+"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "x \\to 0^+", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "be2ef225-9a59-4d75-baf4-e00440c558bb"]
}], "Text",
 CellChangeTimes->{{3.8871240822626524`*^9, 
  3.8871241293326225`*^9}},ExpressionUUID->"7680ad0d-cfff-4df9-bb44-\
111373fde2bd"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Integrate", "[", " ", 
   RowBox[{
    RowBox[{"Collect", "[", 
     RowBox[{
      RowBox[{"Series", "[", 
       RowBox[{
        FractionBox[
         RowBox[{"E", "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "x"}], "*", "t"}], ")"}]}], 
         RowBox[{"1", "+", 
          SuperscriptBox["t", "2"]}]], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0", ",", "5"}], "}"}]}], "]"}], ",", "x"}], "]"}],
     ",", 
    RowBox[{"{", 
     RowBox[{"t", ",", "0", ",", "1"}], "}"}]}], "]"}], "//", 
  "Expand"}]], "Input",
 CellChangeTimes->{{3.887124247194579*^9, 3.887124300548394*^9}, {
  3.887205896288494*^9, 3.887205898287806*^9}, {3.887207069580733*^9, 
