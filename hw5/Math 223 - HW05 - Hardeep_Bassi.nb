Cell[TextData[{
 "Find the leading behavior of ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[Integral]", "0", "1"], 
         FractionBox[
          SuperscriptBox[
           StyleBox["e", "TI"], 
           RowBox[{"-", 
             StyleBox["x", "TI"], 
             StyleBox["t", "TI"]}]], 
          RowBox[{"1", "+", 
            SuperscriptBox[
             StyleBox["t", "TI"], "2"]}]], 
         StyleBox["d", "TI"], 
         StyleBox["t", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\int_{0}^{1} \\frac{e^{-xt}}{1+t^2}dt", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "775b8661-2fe6-4b81-aea1-26cca3f5a883"],
 " as ",
