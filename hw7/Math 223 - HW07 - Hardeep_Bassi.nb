Cell[CellGroupData[{

Cell["Problem 4", "Section",
 CellChangeTimes->{{3.888413598925548*^9, 3.888413601926664*^9}, {
  3.888498792962681*^9, 
  3.888498803439889*^9}},ExpressionUUID->"5e16f087-82b6-43a4-aa4c-\
897e186a99ad"],

Cell[TextData[{
 "The ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["L", "TI"], 
        StyleBox["p", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "L_p", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4fcc0955-22c0-443e-90bd-248bc4f9e1f6"],
 " norm of a function ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["g", "TI"], TraditionalForm], "errors" -> {}, "input" -> "g", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "403b3fef-e2e6-42a6-b357-d1091e8c6bb1"],
 " is given by ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"||", 
         StyleBox["g", "TI"], 
         SubscriptBox["||", 
          StyleBox["p", "TI"]], "\[LongEqual]", 
         SuperscriptBox[
          RowBox[{"(", 
            RowBox[{
              StyleBox["I", "TI"], "(", 
              StyleBox["p", "TI"], ")"}], ")"}], 
          FractionBox["1", 
           StyleBox["p", "TI"]]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "||g||_p = (I(p))^{\\frac{1}{p}}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b9a71449-32f2-4fea-a176-1eb511f7d929"],
 " where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["I", "TI"], 
         RowBox[{"(", 
           StyleBox["p", "TI"], ")"}], "\[LongEqual]", 
         SubsuperscriptBox["\[Integral]", 
          StyleBox["a", "TI"], 
          StyleBox["b", "TI"]], "|", 
         StyleBox["g", "TI"], 
         RowBox[{"(", 
           StyleBox["t", "TI"], ")"}], 
         SuperscriptBox["|", 
          StyleBox["p", "TI"]], 
         StyleBox["d", "TI"], 
         StyleBox["t", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "I(p) = \\int_{a}^{b}|g(t)|^pdt", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0c1fefc3-92c5-4420-aead-031d342f25cb"],
 ".  Assuming that ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"|", 
         StyleBox["g", "TI"], 
         RowBox[{"(", 
           StyleBox["t", "TI"], ")"}], "|", "\[Element]", 
         SuperscriptBox[
          StyleBox["C", "TI"], "4"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "|g(t)| \\in C^4", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4821f472-953e-46a3-8992-92edb7a96529"],
 " and that it attains its unique maximum on ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["t", "TI"], "\[LongEqual]", 
         StyleBox["c", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "t = c", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "fad531fa-50f5-4dfb-9bea-09a5ed538cc5"],
 " inside ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"[", 
         RowBox[{
           StyleBox["a", "TI"], ",", 
           StyleBox["b", "TI"]}], "]"}], TraditionalForm], "errors" -> {}, 
     "input" -> "[a,b]", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "7edc74fc-48db-4e6f-9625-02ad79eb768c"],
 " with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["g", "TI"], 
         RowBox[{"(", 
           StyleBox["c", "TI"], ")"}], "\[NotEqual]", "0"}], TraditionalForm],
      "errors" -> {}, "input" -> "g(c) \\neq 0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "571e0a66-8b7d-4b3e-a9cb-a564048a737d"],
 ", use Laplace\[CloseCurlyQuote]s method to show that the ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["L", "TI"], 
        StyleBox["p", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "L_p", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "075ac5e6-cfb9-45b5-9523-e7c53fdeec21"],
 " norm converges to the maximum norm as ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["p", "TI"], "\[Rule]", "\[Infinity]"}], TraditionalForm], 
     "errors" -> {}, "input" -> "p \\to \\infty", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e6f1932d-fccf-44ad-861c-1e3028d5691f"],
 "."
}], "Text",
 CellChangeTimes->{{3.888498809540887*^9, 
  3.8884989686651726`*^9}},ExpressionUUID->"3ae78274-569a-4f8d-944b-\
a219acceb490"],

Cell[TextData[{
 "To get this in the form to use Laplace\[CloseCurlyQuote]s method, we can \
reformulate the norm operation as:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"||", 
         StyleBox["g", "TI"], 
         SubsuperscriptBox["||", 
          StyleBox["p", "TI"], 
          StyleBox["p", "TI"]], "\[LongEqual]", 
         SuperscriptBox[
          RowBox[{"(", 
            RowBox[{
              SubsuperscriptBox["\[Integral]", 
