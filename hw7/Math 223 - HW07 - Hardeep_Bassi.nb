Cell[TextData[{
 "Find the first two terms of the asymptotic expansion of ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[Integral]", "5", "9"], 
         FractionBox[
          SuperscriptBox[
           StyleBox["e", "TI"], 
           RowBox[{"-", 
             StyleBox["x", "TI"], 
             StyleBox["t", "TI"]}]], 
          StyleBox["t", "TI"]], 
         StyleBox["d", "TI"], 
         StyleBox["t", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\int_{5}^{9}\\frac{e^{-xt}}{t}dt", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "aaebef5c-ead1-44a3-8ebd-3534cce861dd"],
 "."
}], "Text",
